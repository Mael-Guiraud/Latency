
@article{chitimalla_5g_nodate,
	title = {5G {Fronthaul} – {Latency} and {Jitter} {Studies} of {CPRI} over {Ethernet}},
	abstract = {Common Public Radio Interface (CPRI) is a successful industry cooperation defining the publicly-available specification for the key internal interface of radio base stations between the Radio Equipment Control (REC) and the Radio Equipment (RE) in the fronthaul of mobile networks. However, CPRI is expensive to deploy, consumes large bandwidth, and currently is statically configured. On the other hand, Ethernet-based mobile fronthaul will be cost-efficient and more-easily reconfigurable. Encapsulating CPRI over Ethernet (CoE) is an attractive solution, but stringent CPRI requirements such as delay and jitter are major challenges that need to be met to make CoE a reality. This study investigates whether CoE can meet delay and jitter requirements by performing FPGA-based Verilog experiments and simulations. Verilog experiment shows that CoE encapsulation with fixed Ethernet frame size requires about tens of microseconds. Numerical experiments show that the proposed scheduling policy of CoE flows on Ethernet can reduce jitter when redundant Ethernet capacity is provided. The reduction in jitter can be as large as one microsecond hence making Ethernet-based mobile fronthaul a credible technology.},
	language = {en},
	journal = {Journal of Optical Communications and Networking},
	author = {Chitimalla, Divya and Kondepu, Koteswararao and Valcarenghi, Luca and Tornatore, Massimo and Mukherjee, Biswanath},
	pages = {10},
	annote = {This article presents a problem very similar to ours, on a "star shaped" architecture. The problem looked at is to minimize the jitter on C-RAN streams. Unlike us, the authors treat only the go and not the return. On the other hand, they allow themselves several different flows of C-RAN, which our algorithms at the moment can not support.The proposed algorithm is a greedy routine that fixes the messages in the period in priority order on the routes. This routine is applied to all possible orders of routes and therefore gives an exponential algo.The results deal on the one hand with the choice of the split and its impact on the range that can be covered: trade off between a "heavy" split that has strong latency constraints but that allows not to waste time in the technical part (encapsulation), and a split "light" which has the opposite characteristics (long encapsulation but low latency constraints therefore greater distance).In the second part, the authors evaluate their algorithm and explain that they can do Zero jitter on lightly loaded networks.
To retain of this article: quotes at the beginning, data and interesting explanations on the CPRI.},
	file = {Chitimalla et al. - 5G Fronthaul – Latency and Jitter Studies of CPRI .pdf:/Users/maelguiraud/Zotero/storage/2UIAILCT/Chitimalla et al. - 5G Fronthaul – Latency and Jitter Studies of CPRI .pdf:application/pdf}
}

